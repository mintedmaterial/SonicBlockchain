{
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "rootDir": "src",
      "outDir": "build",
      "lib": [
        "es2020"
      ],
      "types": [
        "@graphprotocol/graph-ts"
      ]
    },
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules",
      "build",
      "generated"
    ]
  }
  